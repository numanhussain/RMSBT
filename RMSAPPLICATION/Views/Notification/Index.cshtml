@model List<RMSCORE.EF.V_Interview>
@{
    ViewBag.title = "Interview Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- Page container -->
<div class="page-container">
    <!-- Page content -->
    <div class="page-content">
        <!-- Main content -->
        <div class="content-wrapper">
            <div style="margin-top:-45px">
                <div class="modal-header">
                    <div class="modal-title">
                        <h3><i class="icon-attachment"></i>&nbsp;&nbsp;Interviews</h3>
                    </div>
                </div>
            </div>
            <!-- Basic datatable -->
            <div class="panel panel-flat">
                <div class="modal-header bg-success-400" style="padding:20px">
                </div>
                <table class="table datatable-basic">
                    <thead>
                        <tr>
                            <th>
                                Interview title
                            </th>
                            <th>
                                Job Title
                            </th>
                            <th>
                                Start Date
                            </th>
                            <th>
                                Start Time
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Title)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.JobTitle)
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => item.StartDate)
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => item.StartTime)
                    </td>
                </tr>
}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/Scripts/Candidate/NotificationManager.js"></script>


